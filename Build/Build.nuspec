<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata minClientVersion="2.5">
    <id>SourceLink.Build</id>
    <version>0.2.1</version>
    <title>SourceLink MSBuild Targets</title>
    <authors>Cameron Taggart</authors>
    <!--<summary></summary>-->
    <description>Provides MSBuild targets for source linking .NET pdb files.</description>
    <projectUrl>https://github.com/ctaggart/SourceLink</projectUrl>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://github.com/ctaggart/SourceLink/raw/50ac7c82a8d4296265808cc773ce02982be304e3/SourceLink128.jpg</iconUrl>
    <dependencies />
  </metadata>
  <files>
    <file src="bin\Release\*.dll" target="build" />
    <file src="bin\Release\*.dll.config" target="build" />
    <file src="bin\Release\*.pdb" target="build" />
    <file src="bin\Release\*.xml" target="build" />
    <file src="SourceLink.Build.targets" target="build" />
  </files>
</package>